        <api_name>x_snc_devtools.GetTransactionRuntime</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns the number of millisenconds passed since the start of this transaction.</description>
        <name>GetTransactionRuntime</name>
        <script><![CDATA[var g_nTransactionRuntime = null;
function GetTransactionRuntime()
{
	var nTimeNow = GetTimeNow();
	if (g_nTransactionRuntime === null)
	{
		g_nTransactionRuntime = nTimeNow;
	}
	return nTimeNow - g_nTransactionRuntime;
}]]></script>
