        <value>var strLog = '';&#13;
function Log(strText)&#13;
{&#13;
	strLog = strLog + "\n" + strText;&#13;
}&#13;
(function(outputs, steps, stepResult, assertEqual)&#13;
{&#13;
	var RenderValue = x_snc_devtools.RenderValue;&#13;
&#13;
	var bResult = true;&#13;
	&#13;
	function TestCase_StringRemoveCharacters(vInput, vInputBlacklist, vExpected)&#13;
	{&#13;
		Log('TestCase_StringRemoveCharacters');&#13;
		Log('Input:           '+RenderValue(vInput));&#13;
		Log('Input Blacklist: '+RenderValue(vInputBlacklist));&#13;
		Log('Expected:        '+RenderValue(vExpected));&#13;
		var vResult = StringRemoveCharacters(vInput,vInputBlacklist);&#13;
		Log('Result  : '+RenderValue(vResult));&#13;
		if (vResult != vExpected)&#13;
		{&#13;
			bResult = false;&#13;
			Log('Test case FAILED');&#13;
		}&#13;
		Log('');&#13;
	}&#13;
	&#13;
	TestCase_StringRemoveCharacters('Camel Case',' ','CamelCase');&#13;
	TestCase_StringRemoveCharacters('Line 1\nLine 2','\n','Line 1Line 2');&#13;
	TestCase_StringRemoveCharacters('I hate CAPITAL letters','hCAPITAL',' ate  letters');&#13;
		&#13;
	stepResult.setOutputMessage(strLog);&#13;
	return bResult;&#13;
	&#13;
})(outputs, steps, stepResult, assertEqual);</value>
