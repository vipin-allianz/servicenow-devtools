        <value>var strLog = '';&#13;
function Log(strText)&#13;
{&#13;
	strLog = strLog + "\n" + strText;&#13;
}&#13;
(function(outputs, steps, stepResult, assertEqual)&#13;
{&#13;
	var bResult = true;&#13;
	&#13;
	function TestCase_RenderValue(vInput, vExpected)&#13;
	{&#13;
		Log('TestCase_RenderValue');&#13;
		Log('GetType(): '+GetType(vInput));&#13;
		Log('Input: '+vInput);&#13;
		Log('Expected: '+vExpected);&#13;
		var vResult = RenderValue(vInput);&#13;
		Log('Result  : '+vResult);&#13;
		if (vResult != vExpected)&#13;
		{&#13;
			bResult = false;&#13;
			Log('Test case FAILED');&#13;
		}&#13;
		Log('');&#13;
	}&#13;
	&#13;
	TestCase_RenderValue(null,'null');&#13;
	TestCase_RenderValue('','');&#13;
	TestCase_RenderValue('Hello World','Hello World');&#13;
	TestCase_RenderValue(42,'42');&#13;
	TestCase_RenderValue([],'[]');&#13;
	TestCase_RenderValue(['Alpha'],'\n[0]: Alpha');&#13;
	TestCase_RenderValue({'Hello': 'World'},'\n["Hello"]: World');&#13;
	&#13;
	var gr = new GlideRecord('x_snc_devtools_test');&#13;
	gr.initialize();&#13;
	gr.value = 'Hello World';&#13;
	TestCase_RenderValue(gr,&#13;
		'\n["sys_meta"]: null'&#13;
		+'\n["sys_id"]: '&#13;
		+'\n["sys_updated_by"]: '&#13;
		+'\n["sys_created_on"]: '&#13;
		+'\n["sys_mod_count"]: 0'&#13;
		+'\n["sys_updated_on"]: '&#13;
		+'\n["sys_tags"]: '&#13;
		+'\n["value"]: Hello World'&#13;
		+'\n["sys_created_by"]: '&#13;
		);&#13;
		&#13;
	stepResult.setOutputMessage(strLog);&#13;
	return bResult;&#13;
	&#13;
})(outputs, steps, stepResult, assertEqual);</value>
